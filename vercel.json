{
    "version": 2,
    "builds": [
      {
        "src": "gadgets/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "gadgets/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles_build"
  }
  